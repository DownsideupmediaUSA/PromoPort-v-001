<ul>
<h5>DJ Dashboard</h5>

  <% if user_signed_in? %>
    <h4>Hi, <%= current_user.username %>!</h4>
  <%end%>
  <%= link_to "Edit Account", edit_user_registration_path %><br>
  <br>

  <div id="latest_track">

      <%= render 'latest_songs'  %>

  </div>


   <div class="comments">
     <ol>
       <!-- for js response -->
     </ol>
  </div>


<br>
<h4>Your Comments:</h4>
  <ul id="user-comment">
    <% current_user.comments.each do |c| %>
      <li>Track Title: "<%= c.song.title %>" oh snap..you said: "<%= c.content %>"</li>
    <% end %>
  </ul>
</ul>
