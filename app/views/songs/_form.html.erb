<%= form_for @song, url: songs_path do|f| %>

  <div class="field"> <%= f.label :title %>:
  <%= f.text_field :title %></div>

  <div class="field">
  Select an artist:
  <%= f.collection_select :artist_id, Artist.all, :id, :artist_name %><br>

  Select a genre:
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>



  <div class="actions">
    <%= f.submit "Create" %>
  </div>
<% end %>
